{
  "name": "@x402/sdk",
  "version": "1.0.0",
  "description": "Official SDK for the X402 autonomous payment protocol on Solana",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "eslint src/",
    "test": "jest"
  },
  "keywords": [
    "x402",
    "solana",
    "payments",
    "ai",
    "agents",
    "autonomous",
    "http-402",
    "web3"
  ],
  "author": "X402 Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/x402onchain/sdk"
  },
  "homepage": "https://x402agent.tech",
  "bugs": {
    "url": "https://github.com/x402onchain/sdk/issues"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.x"
  },
  "peerDependenciesMeta": {
    "@solana/web3.js": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.x",
    "express": "^4.18.2",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
